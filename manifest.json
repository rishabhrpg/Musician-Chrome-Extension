{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Musician",
  "description": "Control your music from any tab and enables keyboard multimedia keys for web media",
  "short_name": "Musician",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "test.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.jiosaavn.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "permissions": [
    "*://*.jiosaavn.com/*",
    "tabs"
  ],
  "manifest_version": 2
}